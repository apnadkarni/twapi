        twapi::end_resource_update $resh -discard
        set reslibh [twapi::load_library $resfile -datafile]
        binary scan [twapi::read_resource $reslibh 9 101 1033] H* hex
        set hex
    } -cleanup {
        twapi::free_library $reslibh
